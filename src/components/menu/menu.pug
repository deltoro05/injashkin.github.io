include ../../data/menu.pug

mixin menu(options = {})

  nav.menu(aria-label='Главное меню')    
    .content 
      input(id='menu__toggle' type='checkbox')
      label(class='menu__btn' for='menu__toggle')
        span    
      ul.menu__box
        each menuItem in menuItems
          li
            a.menu__item(href=menuItem.href)= menuItem.text
